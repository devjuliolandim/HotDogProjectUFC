package SistemaDeVendasDeCachorroQuenteUFC;

public class Venda {

	private Aluno aluno;
	private CachorroQuente cachorroquente;
	private Bebida bebida;
	
	public Venda(Aluno aluno, CachorroQuente cachorroquente, Bebida bebida) {
		super();
		this.aluno = aluno;
		this.cachorroquente = cachorroquente;
		this.bebida = bebida;
	}


	public Bebida getBebida() {
		return bebida;
	}
	
	public void setBebida(Bebida bebida) {
		this.bebida = bebida;
	}
	
	public Aluno getAluno() {
		return aluno;
	}


	public void setAluno(Aluno aluno) {
		this.aluno = aluno;
	}


	public CachorroQuente getCachorroquente() {
		return cachorroquente;
	}


	public void setCachorroquente(CachorroQuente cachorroquente) {
		this.cachorroquente = cachorroquente;
	}
	
	
	
	
	
}
